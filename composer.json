{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5",
        "silex/silex": "~1.3",
        "silex/web-profiler": "*",
        "symfony/browser-kit": "*",
        "symfony/class-loader": "*",
        "symfony/config": "*",
        "symfony/console": "*",
        "symfony/css-selector": "*",
        "symfony/debug": "*",
        "symfony/finder": "*",
        "symfony/form": "*",
        "symfony/monolog-bridge": "*",
        "symfony/process": "*",
        "symfony/security": "*",
        "symfony/translation": "*",
        "symfony/twig-bridge": "*",
        "symfony/validator": "*",
        "fzaninotto/faker":"*"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
